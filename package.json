{
  "engines": {
    "composer": "^0.19.9"
  },
  "name": "banana-network",
  "version": "0.0.14",
  "description": "banana",
  "scripts": {
    "start": "npm run build && composer network install -a dist/banana-network.bna -c PeerAdmin@hlfv1 && composer network start -c PeerAdmin@hlfv1 -n banana-network -A admin -S adminpw -V $npm_package_version",
    "upgrade": "npm run build && composer network install -a dist/banana-network.bna -c PeerAdmin@hlfv1 && composer network upgrade -c PeerAdmin@hlfv1 -n banana-network -V $npm_package_version",
    "build": "mkdir -p ./dist && composer archive create -t dir -n . -a ./dist/banana-network.bna && composer generator docs -a ./dist/banana-network.bna -o ./docs",
    "prepublish": "npm run build",
    "pretest": "npm run lint",
    "lint": "eslint .",
    "test": "nyc mocha -t 0 test/*.js && cucumber-js"
  },
  "keywords": [
    "composer",
    "composer-network"
  ],
  "author": "toan",
  "email": "toancong",
  "license": "Apache-2.0",
  "devDependencies": {
    "@satazor/eslint-config": "^3.2.0",
    "chai": "latest",
    "chai-as-promised": "latest",
    "composer-admin": "^0.19.9",
    "composer-cli": "^0.19.9",
    "composer-client": "^0.19.9",
    "composer-common": "^0.19.9",
    "composer-connector-embedded": "^0.19.9",
    "composer-cucumber-steps": "^0.19.9",
    "cucumber": "^2.2.0",
    "escope": "^3.6.0",
    "eslint": "latest",
    "eslint-config-cssnano": "^3.1.3",
    "eslint-config-logux": "^23.0.0",
    "eslint-config-postcss": "^3.0.1",
    "eslint-config-standard": "^11.0.0",
    "eslint-plugin-es5": "^1.3.1",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-jest": "^21.17.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.8.0",
    "eslint-plugin-security": "^1.4.0",
    "eslint-plugin-standard": "^3.1.0",
    "mkdirp": "latest",
    "mocha": "latest",
    "nyc": "^12.0.2"
  }
}
